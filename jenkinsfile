pipeline {
    agent {
        node {
            label 'maven'
        }
    }
environment {
    PATH = "/opt/apache-maven-3.9.5/bin/mvn:$PATH"
}    

    stages{
        stage("build"{
            steps {
                sh 'mvn clean deploy'
            }
        })
    }


    stages {
        stage('clone-code') {
            steps {
                git branch: 'main', url: 'https://github.com/prafulpatel16/t1-ttrend.git'
            }
        }
    }
}

